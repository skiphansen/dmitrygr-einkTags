			<F N="../firmware/board/chroma29r/board.h"/>
			<F N="../firmware/board/boardCommon.h"/>
			<F N="../firmware/soc/cc111x/cc111x.h"/>
			<F N="../firmware/cpu/8051/cpu.h"/>
