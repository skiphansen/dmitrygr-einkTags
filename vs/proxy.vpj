		</Folder>
		<Folder
			Name="Makefiles"
			Filters="">
			<F
				N="../firmware/make/common.mk"
				Type="Makefile"/>
			<F
				N="../firmware/proxy/Makefile"
				Type="Makefile"/>
			<F
				N="../firmware/make/rules.mk"
				Type="Makefile"/>
		</Folder>
		<Folder
			Name="Other Files"
			Filters="">
			<F N="../chroma_shell/.gitignore"/>
			<F N="/cc1xxx/docs/notes.txt"/>
		</Folder>
	</Files>
</Project>
